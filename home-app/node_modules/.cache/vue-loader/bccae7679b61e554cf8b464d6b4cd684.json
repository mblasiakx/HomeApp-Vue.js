 class=\"bill-input\">\r\n      <input type=\"number\" placeholder=\"price\" v-model=\"Bill.price\" />\r\n    </div>\r\n    <div class=\"bill-input\" >\r\n      <input type=\"date\" v-model=\"Bill.billDate\" />\r\n    </div>\r\n\r\n    <div class=\"button-list\">\r\n  \r\n    <button @click=\"ShowMembers\" >Members</button>\r\n    <button @click=\"AddBill\" >Add</button>\r\n    </div>\r\n  </div>\r\n\r\n  \r\n</template>\r\n\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default{\r\n  props:['MembersToSplit','MembersCountValue'],\r\n  data() {\r\n    return{\r\n      Bill:{\r\n        id:\"\",\r\n        email:\"\",\r\n        title:\"\",\r\n        price:\"\",\r\n        billDate:\"\",\r\n        members:\"\",\r\n        owMeCount: \"\",\r\n        membersValues:[]\r\n\r\n      },\r\n      debt:0,\r\n      showMembers:true,\r\n      hiddenMembers:false\r\n    }\r\n  },\r\n\r\n  methods:{\r\n    async AddBill(){\r\n         const loadedEmail =  localStorage.getItem('emailStore')\r\n            this.Bill.email = loadedEmail\r\n            this.Bill.members = this.MembersToSplit\r\n            this.Bill.membersValues.push(this.MembersCountValue)\r\n          if(this.MembersCountValue.includes(loadedEmail)){\r\n            this.Bill.owMeCount = (this.Bill.price/(this.Bill.members)) *(this.Bill.members - 1)\r\n          }else{\r\n          this.Bill.owMeCount = this.Bill.price\r\n          }\r\n\r\n           \r\n            let resp = await axios.post('http://localhost:4000/app/split', this.Bill)\r\n             this.$emit('GetBill', resp.data);\r\n              this.Bill={\r\n                  title:\"\",\r\n                  price:\"\",\r\n                  billDate:\"\",\r\n              }\r\n\r\n    },\r\n\r\n    ShowMembers(){\r\n        this.$emit(\"ShowMembers\");\r\n    },\r\n\r\n    \r\n  },\r\n\r\n}\r\n</script>\r\n\r\n\r\n\r\n\r\n\r\n<style scoped>\r\n\r\n.Panel{\r\ndisplay: flex;\r\nalign-content: center;\r\nalign-items: center;\r\n}\r\n\r\n\r\n.add-bill-panel{\r\n  width: 100%;\r\n  height: 100%;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n\r\n}\r\n\r\n.add-bill-panel input{\r\n margin: 0 15px;\r\n  width: 10rem;\r\n  height: 2rem;\r\n}\r\n\r\n.button-list{\r\n    width: 15%;\r\n    display: flex;\r\n    justify-content: space-between;\r\n}\r\n\r\n.button-list button{\r\n  background: crimson;\r\n  color: darkgray;\r\n  width: 4.5rem;\r\n  height: 2rem;\r\n}\r\n\r\n.MembersPanel{\r\n  background: brown;\r\n  height: 25rem;\r\n  width: 20rem;\r\n  z-index: 2;\r\n\r\n  \r\n}\r\n\r\n\r\n\r\n</style>"]}]}