          console.log(\"some error during add\");\r\n                console.log(resp);\r\n            }\r\n          }\r\n    },\r\n\r\n    \r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.add-panel{\r\n    height: 50%;\r\n    display: flex;\r\n    justify-content: center;\r\n}\r\n\r\n.add-panel input{\r\n    width: 18rem;\r\n}\r\n\r\n.add-panel button{\r\n    width: 4.5rem;\r\n    background: rgba(33, 187, 192, 0.4);\r\n    color: rgba(255, 245, 245, 0.918);\r\n}\r\n</style>\r\n"]}]}