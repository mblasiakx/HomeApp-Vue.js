rsDetails from './MembersDetails.vue'\r\nexport default{\r\n\r\n    components:{\r\n        AddBill,\r\n        MyBills,\r\n        MembersDetails\r\n       \r\n    },\r\n    data() {\r\n        return{\r\n       BillsList:[],\r\n       debtToMe:0,\r\n       MembersToSplit: null,\r\n       MembersCountValue:null\r\n        }\r\n \r\n    },\r\n \r\n    methods:{\r\n        GetBill(Bill){\r\n        this.BillsList.push(Bill) \r\n        Bill.membersValues = this.MembersCountValue\r\n     \r\n                if(Bill.membersValues.includes('1'))\r\n                {\r\n                this.debtToMe =  this.debtToMe + (Bill.price/(this.MembersToSplit))*(this.MembersToSplit-1)\r\n                 }\r\n                else{\r\n                     this.debtToMe =  this.debtToMe + Bill.price\r\n                }\r\n   \r\n       \r\n        },\r\n\r\n        GetMembers(MembersCount,MembersCountValue){\r\n        this.MembersToSplit = MembersCount;\r\n        this.MembersCountValue= MembersCountValue\r\n        },\r\n\r\n        ShowMembers(){\r\n            this.$refs.MembersDetails.ShowMembers()\r\n        },\r\n\r\n        RemoveBill(bill){\r\n         \r\n            if(bill.membersValues.includes('1'))\r\n            {\r\n                this.debtToMe = this.debtToMe - ((bill.price/bill.members) * (bill.members-1))\r\n            }\r\n\r\n            else{\r\n                this.debtToMe = this.debtToMe - bill.price\r\n            }\r\n           \r\n            this.BillsList.splice(this.BillsList.indexOf(bill), 1);\r\n           \r\n            \r\n        }\r\n\r\n    }\r\n}\r\n</script>\r\n\r\n\r\n<style scoped>\r\n.SplitPanel{\r\n    width: 100%;\r\n    height: 80%;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: flex-start;\r\n    align-items: center;\r\n\r\n}\r\n\r\n.add-bills{\r\n height: 20%;\r\n width: 60%;\r\n\r\n}\r\n\r\n.bills-list{\r\n    height:40%;\r\n    width: 90%;\r\n    text-align: center;\r\n \r\n}\r\n\r\n</style>"],"so