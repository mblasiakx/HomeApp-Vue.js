>\r\n\r\nimport axios from 'axios'\r\nexport default{\r\n    props:[\"BillsList\", \"debtToMe\"],\r\n\r\n    data() {\r\n        return{\r\n   \r\n        }\r\n    },\r\n\r\n    methods:{\r\n         async deleteBill(bill) {\r\n         //this.$emit(\"RemoveBill\", bill);\r\n          let oneBill = this.$props.BillsList[bill];\r\n          await axios.delete(`http://localhost:4000/app/split/${oneBill._id}`);\r\n          this.$props.BillsList.splice(bill,1);\r\n    },\r\n\r\n    \r\n  \r\n    },\r\n\r\n   \r\n    \r\n\r\n\r\n}\r\n</script>\r\n\r\n\r\n<style scoped>\r\n.active-bills-panel{\r\n  list-style: none;\r\n  display: flex;\r\n  flex-direction: row;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n \r\n}\r\n.all-bils-count{\r\n  background: brown;\r\n  font-size: 1.2rem;\r\n  font-weight: bold;\r\n}\r\n\r\n.one-bill {\r\n  margin: 0 10px;\r\n \r\n}\r\n.one-bill ul li{\r\n  list-style: none;\r\n  padding:0 250px;\r\n}\r\n\r\n.one-bill span{\r\n  color: rgb(217, 236, 44);\r\n   font-size: 1.5em;\r\n  font-weight: bold;\r\n}\r\n\r\n.one-bill span:nth-of-type(2){\r\n  color: rgb(156, 8, 8);\r\n   font-size: 1.5em;\r\n  font-weight: bold;\r\n}\r\n\r\n\r\n.one-bill span:nth-of-type(3){\r\n  color: rgb(70, 4, 59);\r\n   font-size: 1.5em;\r\n  font-weight: bold;\r\n}\r\n\r\n\r\n\r\n</style>"],"sourceRoot":""}]}