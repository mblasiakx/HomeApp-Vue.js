se.data.find(a=>a.flatId === tmp1 /* && a.email === \"bla@pl\" && a.password === \"bla3\"* /)){\r\n                     console.log(\"jest\")\r\n                    // this.pageActivated = !this.pageActivated\r\n                 }\r\n             })\r\n             .catch(function(error){\r\n                 console.log(error)\r\n             })\r\n             */\r\n             if(this.pageActivated){\r\n                 this.storeLoginData()\r\n                this.$router.push('/productPanel'); \r\n             }\r\n              \r\n\r\n           \r\n        },\r\n\r\n        SignIn(){\r\n            const registered={\r\n            flatId: this.flatId,\r\n            email: this.email,\r\n            password: this.password,\r\n            confirmPassword:this.confirmPassword,\r\n            }\r\n            axios.post('http://localhost:4000/app/signup', registered)\r\n            \r\n            this.flatId =''\r\n            this.email ='',\r\n            this.password ='',\r\n            this.confirmPassword = ''\r\n\r\n        },\r\n\r\n        storeLoginData(){\r\n\r\n          const flatIdStore =  this.flatIdLogin\r\n          const emailStore = this.emailLogin\r\n            localStorage.setItem('flatIdStore',flatIdStore)\r\n            localStorage.setItem('emailStore',emailStore)\r\n        }\r\n        \r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n\r\n.container {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    height: 50%;\r\n    width: 25%;\r\n    margin: 1rem;\r\n    padding: 2rem;\r\n    box-shadow: 0 50px 50px rgba(0, 0, 0, 0.2);\r\n    border-radius: 4px;\r\n    background: #c5e5e9;\r\n}\r\n.form{\r\n  \r\n    width: 80%;\r\n}\r\n\r\n.title{\r\n    text-align: center;\r\n}\r\n\r\n\r\n\r\n.form-hidden{\r\n    display: none;\r\n}\r\n\r\n.message{\r\n   text-align: center;\r\n    margin-bottom: 1rem; \r\n}\r\n\r\n.message-error{\r\n   text-align: center;\r\n    margin-bottom: 1rem; \r\n}\r\n\r\